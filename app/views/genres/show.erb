<h1>Genre: <%= @genre.name %></h1>


<h3>Artists by Genre:</h3>

<ol>
<% @genre.artists.each do |artist| %>
    <li><a href="/artists/<%= genre.slug %>"><%= genre.name %> </a></li>
<% end %>
</ol>


<h3>Songs by Genre: </h3>

<ol>
<% @genre.songs.each do |song| %>
    <li><a href="/songs/<%= genre.slug %>"><%= genre.name %> </a></li>
<% end %>
</ol>
